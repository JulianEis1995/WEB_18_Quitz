SELECT pid FROM tscoreboard ORDER BY pid DESC

SELECT question, a1, a2, a3, a4, ra From tquestions

SELECT question From tquestions Where fid > 10

DELETE questions FROM tquestions Where fid = 5

Insert into tquestions (fid, question, a1, a2, a3, a4, ra)
Values(1, `Welcher Tag ist heute`, `Montag´, ´Dienstag´,  ´Mittwoch´, ´Donnerstag´, ´Dienstag´)
